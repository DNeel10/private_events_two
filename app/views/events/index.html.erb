<h1>Find an Event</h1>
<div>
  <div>
    <h2>Upcoming Events</h2>
    <% @events.upcoming.each do |event| %>
      <%= link_to event.title, event %>
    <% end %>
  </div>

  <div>
    <h2>Past Events</h2>
    <% @events.past.each do |event| %>
      <%= link_to event.title, event %>
    <% end %>
  </div>

</div><br><br>

<%= link_to "Create new Event", new_event_path %>